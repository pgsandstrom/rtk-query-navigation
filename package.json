{
  "name": "rtk-query-navigation",
  "displayName": "RTK Query Navigation",
  "description": "Improved go to definition in RTK Query",
  "version": "1.1.0",
  "publisher": "codeandstuff",
  "license": "MIT",
  "icon": "logo/logo.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "RTK",
    "RTK Query",
    "go to definition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pgsandstrom/rtk-go-to-definition"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "my-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --fix --no-warn-ignored",
      "prettier --write"
    ]
  },
  "scripts": {
    "compile-and-load-plugin": "npm run compile && rm package-lock.json && rm -rf node_modules/my-plugin && npm install --force",
    "compile": "npm run build --prefix my-plugin && tsc -p ./",
    "format": "prettier --write \"{,!(.vscode-test|out|node_modules)/**/}!(package-lock).{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint --ext .ts --ext .tsx --fix src my-plugin/src",
    "package": "vsce package",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && npm run typecheck && npm run test",
    "prepare": "husky",
    "test": "npm run test --prefix my-plugin",
    "typecheck": "npm run typecheck --prefix my-plugin && tsc --noEmit",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "dependencies": {
    "my-plugin": "file:./my-plugin/my-plugin-0.0.1.tgz"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "22.19.0",
    "@types/vscode": "1.105.0",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "@vscode/test-cli": "0.0.12",
    "@vscode/test-electron": "2.5.2",
    "@vscode/vsce": "3.6.2",
    "eslint": "9.36.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  }
}
