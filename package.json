{
  "name": "rtk-go-to-definition",
  "displayName": "rtk-go-to-definition",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "RTK",
    "RTK Query"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pgsandstrom/rtk-go-to-definition"
  },
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "my-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "scripts": {
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "format": "prettier --write \"{,!(.vscode|.vscode-test|out|node_modules)/**/}!(package-lock).{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "eslint src",
    "package": "vsce package",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "webpack --mode production --devtool hidden-source-map",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "dependencies": {
    "my-plugin": "file:./my-plugin"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "3.6.2",
    "eslint": "^9.36.0",
    "prettier": "3.6.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  }
}
